{
  "name": "stravels",
  "version": "0.0.4",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "clean:packages": "rm -rf node_modules && yarn && npm shrinkwrap",
    "clean:mapbox": "react-native unlink react-native-mapbox-gl && npm run clean:packages && react-native link",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "codepush:staging:android": "code-push release-react Stravels-Android android -d Staging",
    "codepush:staging:ios": "code-push release-react Stravels-iOS ios -d Staging",
    "codepush:staging": "npm run codepush:staging:android && npm run codepush:staging:ios",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install:release": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:install:staging": "cd android && ./gradlew assembleReleaseStaging && ./gradlew installReleaseStaging",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "prepush": "npm run git-hook",
    "git-hook": "npm run lint -s && npm run test -s",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "ios": "react-native run-ios",
    "ios:debug": "react-native run-ios --configuration Debug",
    "ios:release": "react-native run-ios --configuration Release",
    "android": "react-native run-android",
    "android:debug": "react-native run-android --configuration Debug",
    "android:release": "react-native run-android --configuration Release"
  },
  "dependencies": {
    "@mapbox/polyline": "^0.2.0",
    "@turf/turf": "^4.7.2",
    "apisauce": "^0.14.0",
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "format-json": "^1.0.3",
    "lodash": "^4.17.2",
    "mobile-center": "^0.9.0",
    "mobile-center-analytics": "^0.9.0",
    "mobile-center-crashes": "^0.9.0",
    "mobile-center-push": "^0.9.0",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "querystringify": "0.0.4",
    "ramda": "^0.24.1",
    "react": "16.0.0-alpha.12",
    "react-native": "^0.47.1",
    "react-native-animatable": "^1.2.3",
    "react-native-code-push": "^5.1.0-beta",
    "react-native-config": "^0.6.0",
    "react-native-device-info": "^0.11.0",
    "react-native-drawer": "^2.3.0",
    "react-native-i18n": "1.0.0",
    "react-native-mapbox-gl": "^5.2.0",
    "react-native-vector-icons": "^4.3.0",
    "react-navigation": "1.0.0-beta.11",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-offline": "^2.0.0",
    "redux-persist": "^4.1.0",
    "redux-saga": "^0.14.3",
    "reduxsauce": "0.4.1",
    "seamless-immutable": "^7.0.1",
    "simplify-js": "^1.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.0",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-native": "^4.0.0",
    "deep-freeze": "^0.0.1",
    "enzyme": "3.0.0-alpha.1",
    "husky": "^0.14.3",
    "jest": "^21.1.0",
    "mockery": "^2.0.0",
    "react-dom": "16.0.0-alpha.12",
    "react-test-renderer": "16.0.0-alpha.12",
    "reactotron-apisauce": "^1.12.2",
    "reactotron-react-native": "^1.12.0",
    "reactotron-redux": "^1.11.1",
    "reactotron-redux-saga": "^1.11.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "tests/setup.js"
    ],
    "setupFiles": [
      "./tests/setup"
    ],
    "preset": "react-native"
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
